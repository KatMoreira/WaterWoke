        EditField_9                matlab.ui.control.NumericEditField
        RecommendedLabel_2         matlab.ui.control.Label
        MinutesDayLabel            matlab.ui.control.Label
        EditField_8                matlab.ui.control.NumericEditField
        RecommendedLabel           matlab.ui.control.Label
        BathLabel                  matlab.ui.control.Label
        HandwashingDishesLabel     matlab.ui.control.Label
        DishwasherLabel            matlab.ui.control.Label
