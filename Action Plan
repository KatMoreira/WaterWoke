    %properties (Access = public)
        %perWeekTot = app2.PerWeekEditField.Value; % Description
    %end
    

    % Callbacks that handle component events
    methods (Access = private)

        % Value changed function: TakingaShowerCheckBox
        function BoxOneChecked(app, event)
            value = app.TakingaShowerCheckBox.Value;
            app.EditField_7.Visible = 'on';
            app.MinutesLabel_4.Visible = 'on';
            if value == 0
               app.EditField_7.Visible = 'off';
               app.MinutesLabel_4.Visible = 'off';
            end
        end

        % Value changed function: RunningaBathCheckBox
        function BoxTwoChecked(app, event)
            value = app.RunningaBathCheckBox.Value;
            app.EditField_2.Visible = 'on';
            app.TimesLabel.Visible = 'on';
            if value == 0
                app.EditField_2.Visible = 'off';
                app.TimesLabel.Visible = 'off'; 
            end
        end

        % Value changed function: FlushingToiletCheckBox
        function BoxThreeChecked(app, event)
            value = app.FlushingToiletCheckBox.Value;
            app.EditField_4.Visible = 'on';
            app.TimesLabel_2.Visible = 'on';
            if value == 0
                app.EditField_4.Visible = 'off';
                app.TimesLabel_2.Visible = 'off';
            end
        end

        % Value changed function: WashingMachineCheckBox
        function BoxFourChecked(app, event)
            value = app.WashingMachineCheckBox.Value;
            app.EditField_5.Visible = 'on';
            app.LoadsLabel.Visible = 'on';
            if value == 0
                app.EditField_5.Visible = 'off';
                app.LoadsLabel.Visible = 'off';
            end
        end

        % Value changed function: DishwasherCheckBox
        function BoxFiveChecked(app, event)
            value = app.DishwasherCheckBox.Value;
            app.EditField_6.Visible = 'on';
            app.LoadsLabel_2.Visible = 'on';
            if value == 0
                app.EditField_6.Visible = 'off';
                app.LoadsLabel_2.Visible = 'off';
            end
        end

        % Value changed function: HygieneCheckBox
        function BoxSixChecked(app, event)
            value = app.HygieneCheckBox.Value;
            app.EditField_3.Visible = 'on';
            app.MinutesLabel_3.Visible = 'on';
            if value == 0
                app.EditField_3.Visible = 'off';
                app.MinutesLabel_3.Visible = 'off';
            end
        end

        % Button pushed function: CalculateButton
        function Calculate(app, event)
            showerMin = app.EditField_7.Value;
            bath = app.EditField_2.Value;
            toilet = app.EditField_4.Value;
            wMachine = app.EditField_5.Value;
            dWasher = app.EditField_6.Value;
            hyg = app.EditField_3.Value;
            
            if app.TakingaShowerCheckBox.Value ~= 0
                showerTotal = 7*2*showerMin;
                if showerTotal <= 105
                    app.Lamp.Color = [0.00 1.00 0.00];
                elseif showerTotal <= 175
                    app.Lamp.Color = [0.75 1.00 0.05];
                elseif showerTotal <= 245
                    app.Lamp.Color = [0.95 1.00 0.05];
                elseif showerTotal <= 315
                    app.Lamp.Color = [1.00 0.75 0.05];
                else
                    app.Lamp.Color = [1.00 0.00 0.00];
                end
            else
                showerTotal = 0;
            end
            
            if app.RunningaBathCheckBox.Value ~= 0
                bathTotal = 36*bath;
                if bath <= 3
                    app.Lamp_4.Color = [0.00 1.00 0.00];
                elseif bath <= 6
                    app.Lamp_4.Color = [0.75 1.00 0.05];
                elseif bath <= 9
                    app.Lamp_4.Color = [0.95 1.00 0.05];
                elseif bath <= 12
                    app.Lamp_4.Color = [1.00 0.75 0.05];
                else
                    app.Lamp_4.Color = [1.00 0.00 0.00];
                end
            else 
                bathTotal = 0;
            end
            
            if app.FlushingToiletCheckBox.Value ~= 0
                toiletTotal = 7*3*toilet;
                if toilet <= 7
                    app.Lamp_2.Color = [0.00 1.00 0.00];
                elseif toilet <= 10
                    app.Lamp_2.Color = [0.75 1.00 0.05];
                elseif toilet <= 13
                    app.Lamp_2.Color = [0.95 1.00 0.05];
                elseif toilet <= 16
                    app.Lamp_2.Color = [1.00 0.75 0.05];
                else
                    app.Lamp_2.Color = [1.00 0.00 0.00];
                end
            else
                toiletTotal = 0;
            end
        
            if app.WashingMachineCheckBox.Value ~= 0
                wMachineTotal = 15*wMachine;
                if wMachine <= 2
                    app.Lamp_5.Color = [0.00 1.00 0.00];
                elseif wMachine <= 5
                    app.Lamp_5.Color = [0.75 1.00 0.05];
                elseif wMachine <= 8
                    app.Lamp_5.Color = [0.95 1.00 0.05];
                elseif wMachine <= 11
                    app.Lamp_5.Color = [1.00 0.75 0.05];
                else
                    app.Lamp_5.Color = [1.00 0.00 0.00];
                end
            else 
                wMachineTotal = 0;
            end
            
            if app.DishwasherCheckBox.Value ~= 0
                dWasherTotal = 8*dWasher;
                if dWasher <= 2
                    app.Lamp_6.Color = [0.00 1.00 0.00];
                elseif dWasher <= 5
                    app.Lamp_6.Color = [0.75 1.00 0.05];
                elseif dWasher <= 8
                    app.Lamp_6.Color = [0.95 1.00 0.05];
                elseif dWasher <= 11
                    app.Lamp_6.Color = [1.00 0.75 0.05];
                else
                    app.Lamp_6.Color = [1.00 0.00 0.00];
                end
            else 
                dWasherTotal =0;
            end
            
            if app.HygieneCheckBox.Value ~= 0
                hygTotal = 7*2*hyg;
                if hyg <= 2
                    app.Lamp_3.Color = [0.00 1.00 0.00];
                elseif hyg <= 5
                    app.Lamp_3.Color = [0.75 1.00 0.05];
                elseif hyg <= 8
                    app.Lamp_3.Color = [0.95 1.00 0.05];
                elseif hyg <= 11
                    app.Lamp_3.Color = [1.00 0.75 0.05];
                else
                    app.Lamp_3.Color = [1.00 0.00 0.00];
                end
            else 
                hygTotal = 0;
            end
            
            app.GalPerWeekEditField.Value = showerTotal + bathTotal ...
            + toiletTotal + wMachineTotal + dWasherTotal + hygTotal;
           
            % labels = repmat({'', '', '', '', '', ''},size(6));
            labels = {'Showers', 'Baths', 'Toilet', 'Washing Machine', 'Dish Washer', 'Hygiene'};
            
            if app.GalPerWeekEditField.Value == 0
                app.TryinputtingvaluestogetresultsLabel.Visible = ('on');
            else
                app.TryinputtingvaluestogetresultsLabel.Visible = ('off');
                pie(app.UIAxes, [showerTotal bathTotal toiletTotal ...
                wMachineTotal dWasherTotal hygTotal], [1 1 1 1 1 1], labels)
            end
            
        end

        % Value changed function: EditField_15
        function Totals(app, event)
            showerMin = app.EditField_7.Value;
            bath = app.EditField_2.Value;
            toilet = app.EditField_4.Value;
            wMachine = app.EditField_5.Value;
            dWasher = app.EditField_6.Value;
            hyg = app.EditField_3.Value;
            
            showerTotal = 7*2*showerMin;
            bathTotal = 36*bath;
            toiletTotal = 7*3*toilet;
            wMachineTotal = 15*wMachine;
            dWasherTotal = 8*dWasher;
            hygTotal = 7*2*hyg;
            
            if app.ShowerCheckBox.Value == 0
                showerTotal = 0;
                app.EditField_8.Value = showerMin;
            end
            
            if app.BathCheckBox.Value ==0
                bathTotal = 0;
                app.EditField_9.Value = bath;
            end
            
            if app.HygienceCheckBox.Value ==0
                hygTotal = 0;
                app.EditField_10.Value = hyg;
            end
            
            if app.FlushingCheckBox.Value == 0
                toiletTotal = 0;
                app.EditField_11.Value = toilet;
            end
            
            if app.WashingMachineCheckBox_2.Value == 0
                wMachineTotal = 0;
                app.EditField_12.Value = wMachine;
            end
            
            if app.DishwasherCheckBox_2.Value == 0
                dWasherTotal = 0;
                app.EditField_13.Value = dWasher;
            end
            
            actionTotal = showertotal + toiletToilet + bathTotal + ...
                hygTotal + wMachineTotal + dWasherTotal;
            V = (app.GalPerWeekEditField.Value * app.percentField.Value) / 100;
            
            pcShower = showerTotal/actionTotal;
            pcToilet = toiletTotal/actionTotal;
            pcHygiene = hygTotal/actionTotal;
            pcBath = bathTotal/actionTotal;
            pcWash = wMachineTotal/actionTotal;
            pcDish = dWasherTotal/actionTotal;
            
            workS = pcShower * V;
            workT = pcToilet * V;
            workH = pcHygiene * V;
            workB = pcBath * V;
            workW = pcWash * V;
            workD = pcDish * V;
            
            app.EditField_8.Value = (showerTotal - workS) / 14;
            app.EditField_9.Value = (toiletTotal - workT) / 21;
            app.EditField_10.Value = (hygTotal - workH) / 14;
            app.EditField_11 = (bathTotal - workB) / 36;
            app.EditField_12 = (wMachineTotal - workW) / 15;
            app.EditField_13 = (dWasherTotal - workD) / 8;
            
            
        end

        % Button pushed function: CreatemyactionplanButton
        function WhenPressed(app, event)
             showerMin = app.EditField_7.Value;
            bath = app.EditField_2.Value;
            toilet = app.EditField_4.Value;
            wMachine = app.EditField_5.Value;
            dWasher = app.EditField_6.Value;
            hyg = app.EditField_3.Value;
            
            showerTotal = 7*2*showerMin;
            bathTotal = 36*bath;
            toiletTotal = 7*3*toilet;
            wMachineTotal = 15*wMachine;
            dWasherTotal = 8*dWasher;
            hygTotal = 7*2*hyg;
            
            if app.ShowerCheckBox.Value == 0
                showerTotal = 0;
                app.EditField_8.Value = showerMin;
            end
            
            if app.BathCheckBox.Value ==0
                bathTotal = 0;
                app.EditField_9.Value = bath;
            end
            
            if app.HygieneCheckBox.Value ==0
                hygTotal = 0;
                app.EditField_10.Value = hyg;
            end
            
            if app.FlushingCheckBox.Value == 0
                toiletTotal = 0;
                app.EditField_11.Value = toilet;
            end
            
            if app.WashingMachineCheckBox_2.Value == 0
                wMachineTotal = 0;
                app.EditField_12.Value = wMachine;
            end
            
            if app.DishwasherCheckBox_2.Value == 0
                dWasherTotal = 0;
                app.EditField_13.Value = dWasher;
            end
            
            actionTotal = showerTotal + toiletTotal + bathTotal + ...
                hygTotal + wMachineTotal + dWasherTotal;
            V = (app.GalPerWeekEditField.Value * app.percentField.Value) / 100;
            
            pcShower = showerTotal/actionTotal;
            pcToilet = toiletTotal/actionTotal;
            pcHygiene = hygTotal/actionTotal;
            pcBath = bathTotal/actionTotal;
            pcWash = wMachineTotal/actionTotal;
            pcDish = dWasherTotal/actionTotal;
            
            workS = pcShower * V;
            workT = pcToilet * V;
            workH = pcHygiene * V;
            workB = pcBath * V;
            workW = pcWash * V;
            workD = pcDish * V;
            
            app.EditField_8.Value = cast(floor((showerTotal - workS) / 14), "uint8");
            app.EditField_9.Value = cast(floor((toiletTotal - workT) / 21), "uint8");
            app.EditField_10.Value = cast(floor((hygTotal - workH) / 14), "uint8");
            app.EditField_11 = cast(floor((bathTotal - workB) / 36), "uint8");
            app.EditField_12 = cast(floor((wMachineTotal - workW) / 15), "uint8");
            app.EditField_13 = cast(floor((dWasherTotal - workD) / 8), "uint8");
            
        end
    end

